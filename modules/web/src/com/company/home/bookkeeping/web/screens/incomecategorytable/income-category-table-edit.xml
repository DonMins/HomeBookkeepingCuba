<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        caption="msg://incomeCategoryTableEdit.caption"
        focusComponent="form"
        messagesPack="com.company.home.bookkeeping.web.screens.incomecategorytable">
    <data>
        <instance id="incomeCategoryTableDc"
                  class="com.company.home.bookkeeping.entity.IncomeCategoryTable">
            <view extends="_local"/>
            <loader/>
        </instance>
    </data>
    <dialogMode height="600"
                width="800"/>
    <layout expand="editActions" spacing="true">
        <form id="form" dataContainer="incomeCategoryTableDc">
            <column width="350px">
                <textArea id="nameCategoryField" property="nameCategory" rows="5"/>
            </column>
        </form>
        <hbox id="editActions" spacing="true">
            <button action="windowCommitAndClose"/>
            <button action="windowClose"/>
        </hbox>
    </layout>
</window>
